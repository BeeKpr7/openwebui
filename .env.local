# Configuration Open WebUI - Développement Local

# Configuration Ollama
OLLAMA_BASE_URL=http://ollama:11434
OLLAMA_PORT=11434

# Configuration Open WebUI
OPENWEBUI_PORT=8080
WEBUI_NAME=Open WebUI - Dev
WEBUI_AUTH=True
ENABLE_SIGNUP=True
DEFAULT_USER_ROLE=user

# Configuration de sécurité
WEBUI_SECRET_KEY=your-secret-key-change-this-in-production

# Configuration des uploads
MAX_UPLOAD_SIZE=25000000

# Configuration du partage communautaire (activé en dev)
ENABLE_COMMUNITY_SHARING=True

# Configuration de debug
DEBUG=True
