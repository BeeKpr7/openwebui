# Configuration OpenWebUI - Environnement Local
# Fichier d'exemple - généré automatiquement par deploy-local.sh

# Ports
OPENWEBUI_PORT=8080
OLLAMA_PORT=11434

# Configuration Ollama (local)
OLLAMA_BASE_URL=http://ollama:11434

# Configuration WebUI
WEBUI_NAME=OpenWebUI Local
WEBUI_AUTH=true
ENABLE_SIGNUP=true
DEFAULT_USER_ROLE=user

# Configuration de développement
ENABLE_COMMUNITY_SHARING=false
MAX_UPLOAD_SIZE=25000000

# Configuration des logs
LOG_LEVEL=INFO

# Configuration optionnelle
ENABLE_OAUTH_SIGNUP=true
ENABLE_LITELLM=true
ENABLE_MODEL_FILTER=true
ENABLE_WEB_SEARCH=true

# Variables additionnelles que vous pouvez personnaliser :
# WEBUI_SECRET_KEY=votre_cle_secrete_unique
# ENABLE_RAG_WEB_SEARCH=false
# ENABLE_SEARCH_QUERY=false
# ENABLE_WEB_BROWSE=false
# ENABLE_IMAGE_GENERATION=false
# AUTOMATIC1111_BASE_URL=http://127.0.0.1:7860
# COMFYUI_BASE_URL=http://127.0.0.1:8188
# OPENAI_API_BASE_URL=https://api.openai.com/v1
# OPENAI_API_KEY=your_openai_api_key_here 