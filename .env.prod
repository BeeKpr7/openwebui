# Configuration OpenWebUI Production
OLLAMA_BASE_URL=http://localhost:11434

# Configuration de l'interface web
WEBUI_NAME=OpenWebUI Production
WEBUI_AUTH=true
ENABLE_SIGNUP=true
DEFAULT_USER_ROLE=user

# Clé secrète - À CHANGER pour votre environnement
WEBUI_SECRET_KEY=your-super-secret-key-here-change-this-in-production

# Configuration des formulaires
ENABLE_LOGIN_FORM=true
ENABLE_OAUTH_SIGNUP=false
ENABLE_COMMUNITY_SHARING=false

# Configuration des uploads
MAX_UPLOAD_SIZE=104857600
MAX_FILE_SIZE=104857600

# Configuration du port pour nginx reverse proxy
OPENWEBUI_PORT=8080 